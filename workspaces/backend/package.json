{
  "name": "@ltvod/backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "node ./dist/index.js",
    "lint": "tslint --project ./ --fix",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "moment": "2.24.0",
    "mysql": "2.17.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@types/express": "4.17.1",
    "@types/mysql": "2.15.7",
    "@types/node": "11.13.21",
    "babel-jest": "24.9.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "ts-loader": "6.2.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-node-externals": "1.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1"
    }
  }
}
